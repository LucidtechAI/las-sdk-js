{
  "name": "@lucidtech/las-sdk",
  "homepage": "https://lucidtech.ai",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "test": "lerna run test",
    "preupgrade-lucidtech": "lerna bootstrap --sort && lerna run --sort compile",
    "upgrade-lucidtech": "lerna run upgrade",
    "postupgrade-lucidtech": "lerna run --sort compile",
    "watch": "lerna run watch",
    "bootstrap": "lerna bootstrap --sort",
    "compile": "lerna run compile",
    "recompile": "lerna clean -y && lerna run compile",
    "build": "lerna clean -y && lerna run compile && jest",
    "lint": "lerna run --no-bail lint",
    "publish": "lerna publish",
    "test:watch": "jest --watch",
    "coverage": "jest --collectCoverage",
    "docs": "typedoc packages/las-sdk-core/src && npx concat-md --decrease-title-levels temp-docs > docs.md && npx rimraf temp-docs"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^12.19.14",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.19.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^26.6.3",
    "jsdom": "^15.1.1",
    "lerna": "^3.16.4",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.14",
    "typedoc-plugin-markdown": "^3.4.0",
    "typescript": "^4.1.3",
    "uuid": "^3.3.3",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "@lucidtech/las-sdk-browser": "file:packages/las-sdk-browser",
    "@lucidtech/las-sdk-node": "file:packages/las-sdk-node",
    "buffer": "^6.0.3"
  }
}
